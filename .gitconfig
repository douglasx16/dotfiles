[user]
	name = Douglas Rodrigues
	email = drodrigues@uoldiveo.com
[push]
	default = simple
[credential]
	helper = cache --timeout=2592000
[http]
	sslverify = false
[core]
	excludesfile = ~/.gitignore
[credential]
	helper = store
[alias]
	aa = add -A
	pull = pull --rebase
	co = checkout
	ci = commit
	st = status
	df = diff
	ds = diff --staged
	br = branch
	lg = log --graph --oneline --decorate --all
	type = cat-file -t
	dump = cat-file -p
	up = fetch -p
